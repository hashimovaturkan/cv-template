@model PersonalSetting

@{
    ViewData["Title"] = "Details";
}


<div class="content-body">
    <!-- row -->
    <div class="container-fluid">
        <div class="table-wrapper">
            <div class="table-title">
                <div class="row">
                    <div class="col-sm-12">
                        <h2 class="text-center">Personal Setting <b>Details</b></h2>
                    </div>
                </div>
            </div>
        </div>
        <table class="table table-striped table-hover">

            <tbody>
                <tr>
                    <th>Name</th>
                    <td>@Model?.Name</td>
                </tr>
                <tr>
                    <th>Age</th>
                    <td>@Model?.Age</td>
                </tr>
                <tr>
                    <th>Career Level</th>
                    <td>@Model?.CareerLevel</td>
                </tr>
                <tr>
                    <th>Degree</th>
                    <td>@Model?.Degree</td>
                </tr>
                <tr>
                    <th>Email</th>
                    <td>@Model?.Email</td>
                </tr>
                <tr>
                    <th>Experience</th>
                    <td>@Model?.Experience</td>
                </tr>
                <tr>
                    <th>Phone</th>
                    <td>@Model?.Phone</td>
                </tr>
                <tr>
                    <th>Location</th>
                    <td>@Model?.Location</td>
                </tr>
                <tr>
                    <th>Fax</th>
                    <td>@Model?.Fax</td>
                </tr>
                <tr>
                    <th>Website</th>
                    <td>@Model?.Website</td>
                </tr>
                <tr>
                    <th>User</th>
                    <td>@Model?.CvTemplateUserId</td>
                </tr>
            </tbody>
        </table>
        <div>
            <a asp-action="Edit" asp-route-id="@Model?.Id" asp-controller="personalsettings" class="edit"><i class="fa fa-edit" data-toggle="tooltip" title="Edit"></i></a>
            |
            <a asp-action="Index">Back to List</a>
        </div>
    </div>
</div>


